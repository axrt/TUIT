<?xml version="1.0"?>
<!DOCTYPE TUITProperties PUBLIC "-//TUIT//TUITProperties/EN" "properties.dtd">
<TUITProperties>
    <DBConnection url="localhost" login="tuit" password="tuit"/>
    <BLASTNPath path="/bin/blastn"/>
    <TMPDir path="/home/alext/Downloads/tmp"/>
    <BLASTNParameters>
        <Database use="nt"/>
        <Expect value="1.0"/>
        <EntrezQuery value="not hybrid not other not uncultured not enrichment not unclassified not uncultivated not unspecified not environmental not metagenomes"/>
        <Remote deligate="yes"/>
        <MaxFilesInBatch value="50"/>
    </BLASTNParameters>
    <SpecificationParameters>
        <CutoffSet rank="species">
            <pIdentCutoff value="97.5"/>
            <QueryCoverageCutoff value="95"/>
            <EvalueRatioCutoff value="100"/>
        </CutoffSet>
    </SpecificationParameters>
</TUITProperties>