<?xml version="1.0"?>
<!DOCTYPE TUITProperties PUBLIC "-//TUIT//TUITProperties/EN" "properties.dtd">
<TUITProperties>
    <DBConnection url="localhost" login="ocular" password="ocular"/>
    <BLASTNPath path="/usr/bin/blastn"/>
    <TMPDir path="/home/alext/Downloads/tmp"/>
    <BLASTNParameters>
        <Database use="nt"/>
        <Expect value="10"/>
        <EntrezQuery value="not uncultured not enrichment not unclassified not uncultivated not unspecified"/>
        <Remote deligate="yes"/>
        <MaxFilesInBatch value="1"/>
    </BLASTNParameters>
    <SpecificationParameters>
        <CutoffSet rank="species">
            <pIdentCutoff value="97.5"/>
            <QueryCoverageCutoff value="95"/>
            <EvalueRatioCutoff value="100"/>
        </CutoffSet>
    </SpecificationParameters>
</TUITProperties>